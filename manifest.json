{
  "manifest_version": 3,
  "name": "Discord Msdel",
  "description":"...",
  "version": "1.0.0",
  "permissions": [
    "tabs",
    "scripting",
    "activeTab",
    "storage",
    "notifications"
  ],

  "content_scripts": [
    {
      "matches": ["https://discord.com/*"],
      "js": ["src/content/contentScript.js"],
      "run_at": "document_start"
    }
  ],

  "host_permissions": [
    "*://*/*"
  ],
  "action": {
	"default_title":"Discord Msdel Popup",
    "default_popup": "src/views/dist/index.html",
    "default_icon": {}
  },
  "background": {
	"service_worker": "src/background/background.js"
  }
}
